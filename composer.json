{
	"name": "robb-j/ss-boilerplate",
	"description": "Boilerplate template to quickly kick off a new SilverStripe project",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/framework": "3.x@stable",
		"i-lateral/silverstripe-siteconfig": "master"
	},
	"extra": {
		"installer-paths": {
			"public_html/{$name}": [
				"silverstripe/framework",
				"i-lateral/silverstripe-siteconfig",
				"phpunit/phpunit"
			]
		}
	},
	"minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "*@stable"
	}
}
